<% provide(:title, 'Inccoming cargo') %>
<h1>Create new line of incoming cargo </h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@cargoinc) do |f| %>
  
        <%= f.label :name %>
        <%= f.text_field :name, class: 'form-control' %>
  
  
        <%= f.label :number %>
        <%= f.text_field :number, class: 'form-control' %>
  
  
        <%= f.submit "Create new line", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>>
